<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <CommunicationType>GRPC</CommunicationType>
  <PortNumber>9092</PortNumber>
  <IPAddress>127.0.0.1</IPAddress>
  <TimestepDurationInSeconds>0.05</TimestepDurationInSeconds>
  <PhyscsSimulationIncrementInSeconds>0.025</PhyscsSimulationIncrementInSeconds>
  <EnableObservationImage>false</EnableObservationImage>
  <SaveObservationImageAsFile>false</SaveObservationImageAsFile>
  <ObservationImageWidth>100</ObservationImageWidth>
  <ObservationImageHeight>100</ObservationImageHeight>
  <ObservationImageEncoding>JPG</ObservationImageEncoding>
  <ObservationImageQuality>50</ObservationImageQuality>
  <ObservationCameras>
    <CameraParameters>
      <CameraPosition>
        <float>0</float>
        <float>4.3</float>
        <float>3.6</float>
      </CameraPosition>
      <CameraRotation>
        <float>125</float>
        <float>0</float>
        <float>180</float>
      </CameraRotation>
    </CameraParameters>
        <CameraParameters>
      <CameraPosition>
        <float>0.5</float>
        <float>0.2</float>
        <float>1.4</float>
      </CameraPosition>
      <CameraRotation>
        <float>180</float>
        <float>0</float>
        <float>180</float>
      </CameraRotation>
    </CameraParameters>
  </ObservationCameras>
  <EnableGrayscale>false</EnableGrayscale>
  <ItemType>CUBE</ItemType>
  <ItemSizeX>0.05</ItemSizeX>
  <ItemSizeY>0.05</ItemSizeY>
  <ItemSizeZ>0.05</ItemSizeZ>
  <TargetSizeX>0.5</TargetSizeX>
  <TargetSizeY>0.001</TargetSizeY>
  <TargetSizeZ>0.5</TargetSizeZ>
  <EnableTargetCollider>true</EnableTargetCollider>
  <TargetColor>#0000FF</TargetColor>
  <ItemMass>1</ItemMass>
  <RandomizeMass>false</RandomizeMass>
  <ItemMassRandomizationRange>0.1</ItemMassRandomizationRange>
  <ItemDynamicFriction>0.6</ItemDynamicFriction>
  <ItemStaticFriction>0.6</ItemStaticFriction>
  <RandomizeItemFriction>false</RandomizeItemFriction>
  <ItemDynamicFrictionRandomizationRange>0.1</ItemDynamicFrictionRandomizationRange>
  <ItemStaticFrictionRandomizationRange>0.1</ItemStaticFrictionRandomizationRange>
  <ItemDrag>2</ItemDrag>
  <EnableEndEffector>true</EnableEndEffector>
  <EndEffectorModel>ROBOTIQ_3F</EndEffectorModel>
  <MaxJointVelocity>10</MaxJointVelocity>
  <RandomizeAppearance>false</RandomizeAppearance>
  <RandomizeEnvironmentPhysics>false</RandomizeEnvironmentPhysics>
  <CameraPositionRandomizationRangeInMeters>0.05</CameraPositionRandomizationRangeInMeters>
  <CameraRotationRandomizationRangeInDegrees>3</CameraRotationRandomizationRangeInDegrees>
  <EnableShadows>false</EnableShadows>
  <ShadowType>Hard</ShadowType>
  <RandomizeLatency>false</RandomizeLatency>
  <RandomizeTorque>false</RandomizeTorque>
